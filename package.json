{
  "name": "eslint-config-cecilia",
  "version": "2.0.0",
  "description": "ESLint configuration for awesome projects",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "test": "jest"
  },
  "files": [
    "index.js",
    "rules"
