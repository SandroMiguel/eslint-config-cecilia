{
  "name": "eslint-config-cecilia",
  "version": "3.1.0",
  "description": "ESLint configuration for awesome projects",
  "type": "module",
  "bin": {
    "cecilia": "./bin/cecilia.mjs",
    "cecilia-debug": "./bin/debug-config.mjs"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "prepare": "husky install"
  },
  "exports": {
    ".": "./eslint.config.js"
  },
  "main": "./eslint.config.js",
  "files": [
    "eslint.config.js",
    "bin",
    "presets",
    "rules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SandroMiguel/eslint-config-cecilia.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "javascript",
    "react",
    "node"
  ],
  "author": {
    "name": "Sandro Miguel Marques",
    "email": "sandromiguel@sandromiguel.com",
    "url": "http://sandromiguel.com"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/jest": "^29.5.14",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "react": "^18.3.1"
  },
  "dependencies": {
    "globals": "^16.2.0"
  }
}
