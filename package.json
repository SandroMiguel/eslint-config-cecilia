{
  "name": "eslint-config-cecilia",
  "version": "2.5.5",
  "description": "ESLint configuration for awesome projects",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "test": "jest",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "files": [
    ".eslintrc.js",
    "index.js",
    "rules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SandroMiguel/eslint-config-cecilia.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "javascript",
    "react",
    "node"
  ],
  "author": {
    "name": "Sandro Miguel Marques",
    "email": "sandromiguel@sandromiguel.com",
    "url": "http://sandromiguel.com"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^27.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "pinst": "^2.1.6",
    "react": "^17.0.2",
    "standard-version": "^9.3.1"
  }
}
