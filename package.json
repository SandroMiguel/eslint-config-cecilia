{
  "name": "eslint-config-cecilia",
  "version": "2.3.0",
  "description": "ESLint configuration for awesome projects",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "test": "jest"
  },
  "files": [
    ".eslintrc.js",
    "index.js",
    "rules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SandroMiguel/eslint-config-cecilia.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "javascript",
    "react",
    "node"
  ],
  "author": {
    "name": "Sandro Miguel Marques",
    "email": "sandromiguel@sandromiguel.com",
    "url": "http://sandromiguel.com"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.16",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.4",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "standard-version": "^9.0.0"
  }
}
